#include <cstdio>

struct C
{
  inline constexpr C() noexcept = default;
  inline C(const C &)
  {
    printf("Copy\n");
  }
  
  // inline ~C() noexcept = default;
};



C f()
{
  C namedC = C{} /* NRVO variable */;
  return C(namedC);
}


C f2()
{
  C namedC = C{};
  C namedC2 = C{namedC} /* NRVO variable */;
  return C(namedC2);
}



int main()
{
  printf("Hello\n");
  C obj = f();
  C obj2 = f2();
}

